<mvc:View controllerName="Month_alert.Month.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<VBox xmlns="sap.m" id="vboxid"> 
							<Button
				      id="jan"
                       text="January"
                       press=".month"/>
                       <Button
				      id="feb"
                       text="February"
                       press=".month"/>
                       <Button
				      id="Mar"
                       text="March"
                       press=".month"/>
                       <Button
				      id="Ap"
                       text="April"
                       press=".month"/>
                       <Button
				      id="May"
                       text="May"
                       press=".month"/>
                       <Button
				      id="jun"
                       text="June"
                       press=".month"/>
                       <Button
				      id="july"
                       text="July"
                       press=".month"/>
                       <Button
				      id="Aug"
                       text="August"
                       press=".month"/>
                       <Button
				      id="Sep"
                       text="September"
                       press=".month"/>
                       <Button
				      id="Oct"
                       text="October"
                       press=".month"/>
                       <Button
				      id="Nov"
                       text="November"
                       press=".month"/>
						<Button
				      id="dec"
                       text="December"
                       press=".month"/>
                       </VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>